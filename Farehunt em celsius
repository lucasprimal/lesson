import java.util.Scanner;

public class Main {
    public static void main(String[] args){
// Farehunt em celsius
        Scanner scan = new Scanner(System.in);
        System.out.println("Entre com a temperatura em Far");
        double f = scan.nextDouble();
        double c = (5 * (f -32) / 9);

        System.out.println("A tem " + f + "F Ã© igual a" + c + " C");

    }
}
